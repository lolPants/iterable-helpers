{
  "name": "iterable-helpers",
  "version": "0.1.0",
  "description": "Various helper functions for IterableIterators",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run check && npm run lint && npm run ava",
    "ava": "nyc --reporter=html --reporter=text ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "check": "npm run build -- --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc --sourceMap",
    "watch": "tsc --watch",
    "dev": "npm run watch -- --sourceMap"
  },
  "repository": "git@github.com:lolPants/iterable-helpers.git",
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.0.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  }
}
